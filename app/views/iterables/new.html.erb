<%= form_for @iterable, :is_header => params[:is_header] do |f| %>
  <legend>Create New Iterable</legend>
  <%= f.label :name, "Name" %>
  <%= f.text_area :name, :rows => "3" %>
  <%= f.hidden_field :is_header, :value => params[:is_header] %>
  <%= f.submit "Start Iterating", :class => "btn btn-medium" %>
<% end %>