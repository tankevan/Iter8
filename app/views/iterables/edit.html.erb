<%= form_for @iterable, :url=>iterable_path(@iterable), :html=>{:method=>:put} do |f| %>
  <legend>Change Iterable Name</legend>
  <%= f.label :name, "New name" %>
  <%= f.text_field :name %>
  <%= f.label :tag_list, "Tags" %>
  <%= f.text_field :tag_list %>
  <%= f.submit "Start Iterating", :class => "btn btn-medium" %>
<% end %>

<%= @tags %>
